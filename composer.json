{
    "name": "chadicus/psr-cache-helper",
    "description": "Utilities to assist with PSR-16 simple cache implementations",
    "keywords": ["psr", "psr16", "cache", "util", "trait"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Gray",
            "email": "chadwickgray@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "chadicus/coding-standard": "^1.2",
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Chadicus\\Psr\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChadicusTest\\Psr\\SimpleCache\\": "tests"
        }
    }
}
