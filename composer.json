{
    "name": "subjective-php/psr-cache-helper",
    "description": "Utilities to assist with PSR-16 simple cache implementations",
    "keywords": ["psr", "psr16", "cache", "util", "trait"],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Chadicus\\Psr\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChadicusTest\\Psr\\SimpleCache\\": "tests"
        }
    }
}
